$('#myModal').html('<%= escape_javascript(render(:partial => "edit")) %>');
$('#myModal').modal();
initDateTimePicker();
$('#from').datepicker("setDate",  new Date(<%= @event.date_from.year %>, <%= @event.date_from.month %>, <%= @event.date_from.day %>));
$('#to').datepicker("setDate", new Date(<%= @event.date_to.year.to_i %>, <%= @event.date_to.month.to_i %>, <%= @event.date_to.day.to_i %>));