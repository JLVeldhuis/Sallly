$('#myModal').html('<%= escape_javascript(render(:partial => "accept")) %>');
$('#myModal').modal();
initDateTimePicker();
$('#from').datepicker("setDate",  new Date(<%= @event.date_from.year %>, <%= @event.date_from.month %>, <%= @event.date_from.day %>));
$('#to').datepicker("setDate", new Date(<%= @event.date_to.year %>, <%= @event.date_to.month %>, <%= @event.date_to.day %>));