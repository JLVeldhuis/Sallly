<table class="profile" summary="Profile information">
	<tr>
		<td class="main">
			<h1>
				<%= @user.name %>
			</h1>
		</td>
		<td class="sidebar round">
			<strong>Name</strong> <%= @user.name %><br />
			<strong>URL</strong> <%= link_to user_url(@user), @user %><br />
            <strong>Settings id:</strong> <%= @user.setting.id %>
		</td>
	</tr>
</table>
